{
   "background": {
      "persistent": false,
      "scripts": [ "bg.js" ]
   },
   "browser_action": {
      "default_icon": "images/icon.png",
      "default_popup": "popup.html",
      "default_title": "Ability Exercise"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content.js", "myscript.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_start"
   } ],
   "description": "Would U like to be More diligent ,more agile,more skilled ? This may help U ",
   "homepage_url": "https://github.com/SharpScriptSunShine/",
   "icons": {
      "128": "images/icon.png",
      "16": "images/icon.png",
      "32": "images/icon.png",
      "48": "images/icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Ability Exercise",
   "options_page": "options.html",
   "permissions": [ "tabs", "activeTab", "https://*/*", "*://*/*" ],
   "version": "1.0.0"
}
